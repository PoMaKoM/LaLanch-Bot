{
  "name": "lala-bot",
  "version": "1.0.0",
  "description": "Bot for ordering from lalalunch",
  "main": "index.js",
  "author": "PoMa_KoM [https://t.me/poma_kom]",
  "license": "MIT",
  "scripts": {
    "build": "del-cli dist && tsc",
    "start": "cross-env NODE_ENV=production ts-node source/index.ts",
    "start:dev": "ts-node-dev source/index.ts  --watch --dev",
    "test": "tsc --sourceMap && xo && nyc ava"
  },
  "type": "commonjs",
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "@edjopato/telegraf-i18n": "^0.2.0",
    "@types/moment": "^2.13.0",
    "@types/moment-timezone": "^0.5.30",
    "cron": "^1.8.2",
    "lodash-id": "^0.14.0",
    "lowdb": "^1.0.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "source-map-support": "^0.5.19",
    "telegraf": "^4.0.0",
    "telegraf-inline-menu": "^6.0.0",
    "telegraf-middleware-console-time": "^2.0.0",
    "telegraf-session-local": "^2.0.0"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^1.0.1",
    "@types/cron": "^1.7.2",
    "@types/lodash": "^4.14.168",
    "@types/lowdb": "^1.0.9",
    "@types/node": "^14.0.5",
    "ava": "^3.0.0",
    "cross-env": "^7.0.3",
    "del-cli": "^3.0.0",
    "nyc": "^15.0.0",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3",
    "xo": "^0.38.1"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "dist/source",
      "source"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/*.test.ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "xo": {
    "rules": {
      "ava/no-ignored-test-files": "off"
    }
  }
}
